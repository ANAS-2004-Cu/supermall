{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/products.jsx", 
    "app/Login.jsx", 
    "app/Register.jsx", 
    "app/singlepage.jsx", 
    "app/Review.jsx", 
    "app/cart.jsx", 
    "components/notifications.jsx", 
    "app/index.jsx", 
    "app/(ProfileTabs)/orders.tsx", 
    "app/DisplayCategories.jsx", 
    "app/Admintabs/Preview.tsx",
    "app/Onboarding.jsx",
    "app/CategorySelection.jsx"
  ]
}
